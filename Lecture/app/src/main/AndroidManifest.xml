        </activity>
        <activity
            android:name=".page.lecture_edit.Page2LectureEdit"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name=".page.login.Page2Login"
            android:screenOrientation="portrait" />
        <activity
            android:name=".page.lecture_search.Page2LectureSearch"
            android:screenOrientation="portrait" />
        <activity
            android:name=".page.lecture.Page2Lecture"
            android:screenOrientation="portrait" />
        <activity
            android:name=".page.setting.Page2Setting"
            android:screenOrientation="portrait" />
        <activity
            android:name=".page.sign_up.Page2SignUp"
