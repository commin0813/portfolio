<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="commin.pro.lectureschedule">

    <application
        android:allowBackup="true"
        android:icon="@drawable/mentern_icon_1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Application">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".page.note.Page2Note"
            android:screenOrientation="portrait" />

        <activity
            android:name=".page.lecture_edit.Page2LectureEdit"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name=".page.lecture.Page2Lecture"
            android:screenOrientation="portrait" />
        <activity
            android:name=".page.lecture_add.Page2LectureAdd"
            android:screenOrientation="portrait" />
        <activity
            android:name=".page.note_view.Page2NoteView"
            android:screenOrientation="portrait" />
        <activity
            android:name=".page.lecture_view.Page2LectureView"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>